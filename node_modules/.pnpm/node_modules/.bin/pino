#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/pino@6.13.4/node_modules/pino/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/pino@6.13.4/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules:/media/jack/Storage/Documents/Projects/node_modules:/media/jack/Storage/Documents/node_modules:/media/jack/Storage/node_modules:/media/jack/node_modules:/media/node_modules:/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/node_modules/pino/node_modules"
else
  export NODE_PATH="$NODE_PATH:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/pino@6.13.4/node_modules/pino/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/pino@6.13.4/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules:/media/jack/Storage/Documents/Projects/node_modules:/media/jack/Storage/Documents/node_modules:/media/jack/Storage/node_modules:/media/jack/node_modules:/media/node_modules:/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/node_modules/pino/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pino/bin.js" "$@"
else
  exec node  "$basedir/../pino/bin.js" "$@"
fi
