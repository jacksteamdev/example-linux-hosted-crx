#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/semver@7.3.5/node_modules/semver/bin/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/semver@7.3.5/node_modules/semver/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/semver@7.3.5/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules:/media/jack/Storage/Documents/Projects/node_modules:/media/jack/Storage/Documents/node_modules:/media/jack/Storage/node_modules:/media/jack/node_modules:/media/node_modules:/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/fastify@3.25.3/node_modules/fastify/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/fastify@3.25.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/semver@7.3.5/node_modules/semver/bin/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/semver@7.3.5/node_modules/semver/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/semver@7.3.5/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules:/media/jack/Storage/Documents/Projects/node_modules:/media/jack/Storage/Documents/node_modules:/media/jack/Storage/node_modules:/media/jack/node_modules:/media/node_modules:/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/fastify@3.25.3/node_modules/fastify/node_modules:/media/jack/Storage/Documents/Projects/example-linux-hosted-crx/node_modules/.pnpm/fastify@3.25.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../semver@7.3.5/node_modules/semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../../../../../semver@7.3.5/node_modules/semver/bin/semver.js" "$@"
fi
